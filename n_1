#include<iostream>
using namespace std;
int count(int n) {
	int b = -1;
	while (n>0){
		n = n / 10;
		b = b + 1;
	}
	return b;
}
int cif(int n) {
	if (n > 0) {
		int a = pow(10, count(n));
		cout << n / a  << " ";
		n = n % a;
		cif(n);
	}
	return 0;
}
int main() {
	int n;
	cin >> n;
	cif(n);
	system("pause");
	return 0;
}
